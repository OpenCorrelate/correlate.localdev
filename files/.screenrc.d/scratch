# Load common
source $HOME/.screenrc

# Load scratch
chdir $SCRATCH_HOME
screen -t scratch

chdir $SRC_HOME
screen -t src

chdir $BUILD_HOME
screen -t build

chdir $INSTALL_HOME
screen -t install

chdir $INSTALL_HOME/bin
screen -t bin

chdir $SCM_HOME/git/revprez/update-envs
screen -t "update-envs"

chdir $SCRATCH_HOME
